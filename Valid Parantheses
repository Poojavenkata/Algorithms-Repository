class Solution:
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """
        paran={'(':')','[':']','{':'}'}
        stack=[]
        last_ele=""
        for i in range(len(s)):
            if ((s[i]=='(') or (s[i]=='[') or (s[i]=='{')):
                stack.append(s[i])
            else:
                if(len(stack) != 0):
                    last_ele=stack.pop()
                if(last_ele in paran and paran[last_ele]==s[i]):
                    continue
                else:
                    return False
        if(len(stack)==0):
            return True
        return False
